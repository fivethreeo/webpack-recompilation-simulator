{
  "name": "webpack-recompilation-simulator",
  "version": "1.0.1",
  "description": "Helper to test and measure webpack recompilations",
  "files": ["lib/index.js"],
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "npm run test",
    "build": "babel src -d lib --presets es2015",
    "pretest": "babel test -d test-es5 --presets es2015",
    "test": "mocha test-es5/index.js",
    "posttest": "semistandard src/**/*.js test/**/*.js",
    "test-es6": "mocha test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jantimon/webpack-recompilation-simulator.git"
  },
  "author": "Jan Nicklas",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "semistandard": "^7.0.5",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "mkdirp": "^0.5.1"
  }
}
