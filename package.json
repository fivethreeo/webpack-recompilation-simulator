{
  "name": "webpack-recompilation-simulator",
  "version": "1.1.1",
  "description": "Helper to test and measure webpack recompilations",
  "files": [
    "lib/index.js"
  ],
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "npm run test",
    "build": "babel src -d lib --presets es2015",
    "pre": "semistandard src/**/*.js test/**/*.js",
    "test": "mocha --compilers js:babel-core/register",
    "test-es6": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jantimon/webpack-recompilation-simulator.git"
  },
  "author": "Jan Nicklas",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "semistandard": "^7.0.5",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "temp-fs": "^0.9.8"
  }
}
